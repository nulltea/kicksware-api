<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <title>@ViewData["Title"] - web_app_service</title>
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" type="text/css" href="~/css/bootstrap4/bootstrap.min.css">
    <link href="~/lib/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="~/css/main_styles.css">
    <link rel="stylesheet" type="text/css" href="~/css/responsive.css">
    @RenderSection("Styles", required: false)
</head>
<body>
    <div class="super_container">
        <header class="header">
            <div class="header_container">
                <div class="header_content">
                    <div class="logo"><a asp-area="" asp-controller="Home" asp-action="Index">Sneaker.</a></div>
                    <nav class="main_nav">
                        <ul>
                            <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                            <li><a asp-area="" asp-controller="Home" asp-action="Index">Profile</a></li>
                            <li><a asp-area="" asp-controller="Shop" asp-action="Products">Shop</a></li>
                            <li class="hassubs">
                                <a asp-area="" asp-controller="Sell" asp-action="AddProduct">Sell</a>
                                <ul>
                                    <li><a asp-area="" asp-controller="Sell" asp-action="AddProduct">Create new product</a></li>
                                    <li><a asp-area="" asp-controller="Sell" asp-action="MyProducts">My products</a></li>
                                </ul>
                            </li>
                            <li><a asp-area="" asp-controller="About" asp-action="About">About</a></li>
                            <li><a asp-area="" asp-controller="Contact" asp-action="Contact">Contacts</a></li>
                        </ul>
                    </nav>
                    <nav class="icon-nav">
                        <ul>
                            <li class="account hassubs">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M12 4a4 4 0 0 1 4 4a4 4 0 0 1-4 4a4 4 0 0 1-4-4a4 4 0 0 1 4-4m0 2a2 2 0 0 0-2 2a2 2 0 0 0 2 2a2 2 0 0 0 2-2a2 2 0 0 0-2-2m0 7c2.67 0 8 1.33 8 4v3H4v-3c0-2.67 5.33-4 8-4m0 1.9c-2.97 0-6.1 1.46-6.1 2.1v1.1h12.2V17c0-.64-3.13-2.1-6.1-2.1z" fill="#1b1b1b" /></svg>
                                <ul>
                                    <li class="button header-button" id="login-btn">
                                        <span>Login</span>
                                    </li>
                                    <li class="button header-button-strong" id="sign-up-btn">
                                        <span>Sing Up</span>
                                    </li>
                                </ul>
                            </li>
                            <li class="favorite">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1024 1024"><path d="M923 283.6a260.04 260.04 0 0 0-56.9-82.8a264.4 264.4 0 0 0-84-55.5A265.34 265.34 0 0 0 679.7 125c-49.3 0-97.4 13.5-139.2 39c-10 6.1-19.5 12.8-28.5 20.1c-9-7.3-18.5-14-28.5-20.1c-41.8-25.5-89.9-39-139.2-39c-35.5 0-69.9 6.8-102.4 20.3c-31.4 13-59.7 31.7-84 55.5a258.44 258.44 0 0 0-56.9 82.8c-13.9 32.3-21 66.6-21 101.9c0 33.3 6.8 68 20.3 103.3c11.3 29.5 27.5 60.1 48.2 91c32.8 48.9 77.9 99.9 133.9 151.6c92.8 85.7 184.7 144.9 188.6 147.3l23.7 15.2c10.5 6.7 24 6.7 34.5 0l23.7-15.2c3.9-2.5 95.7-61.6 188.6-147.3c56-51.7 101.1-102.7 133.9-151.6c20.7-30.9 37-61.5 48.2-91c13.5-35.3 20.3-70 20.3-103.3c.1-35.3-7-69.6-20.9-101.9zM512 814.8S156 586.7 156 385.5C156 283.6 240.3 201 344.3 201c73.1 0 136.5 40.8 167.7 100.4C543.2 241.8 606.6 201 679.7 201c104 0 188.3 82.6 188.3 184.5c0 201.2-356 429.3-356 429.3z" fill="#1b1b1b" /></svg>
                            </li>
                            <li class="search">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M10 18a7.952 7.952 0 0 0 4.897-1.688l4.396 4.396l1.414-1.414l-4.396-4.396A7.952 7.952 0 0 0 18 10c0-4.411-3.589-8-8-8s-8 3.589-8 8s3.589 8 8 8zm0-14c3.309 0 6 2.691 6 6s-2.691 6-6 6s-6-2.691-6-6s2.691-6 6-6z" fill="#626262" /><path d="M11.412 8.586c.379.38.588.882.588 1.414h2a3.977 3.977 0 0 0-1.174-2.828c-1.514-1.512-4.139-1.512-5.652 0l1.412 1.416c.76-.758 2.07-.756 2.826-.002z" fill="#626262" /></svg>
                            </li>
                        </ul>
                    </nav>
                    <div class="header_extra ml-auto">
                        <div class="hamburger"><i class="fa fa-bars" aria-hidden="true"></i></div>
                    </div>
                </div>
            </div>

            <!-- Search Panel -->
            <div class="search_panel trans_300">
                <div class="container">
                    <div class="row">
                        <div class="col">
                            <div class="search_panel_content d-flex flex-row align-items-center justify-content-end">
                                <form action="#">
                                    <input type="text" class="search_input" placeholder="Search" required="required">
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        <!-- Menu -->
        <div class="menu menu_item trans_300">
            <div class="menu_container menu_item">
                <div class="page_menu_content menu_item">
                    <div class="page_menu_search menu_item">
                        <form action="#">
                            <input type="search" required="required" class="page_menu_search_input menu_item" placeholder="Search for products...">
                        </form>
                    </div>
                    <ul class="page_menu_nav menu_item">
                        <li class="page_menu_item menu_item"><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                        <li class="page_menu_item menu_item"><a asp-area="" asp-controller="Home" asp-action="Index">Profile</a></li>
                        <li class="page_menu_item menu_item"><a asp-area="" asp-controller="Shop" asp-action="Products">Shop</a></li>
                        <li class="page_menu_item menu_item"><a asp-area="" asp-controller="Sell" asp-action="AddProduct">Sell</a></li>
                        <li class="page_menu_item menu_item"><a asp-area="" asp-controller="Home" asp-action="Index">About</a></li>
                        <li class="page_menu_item menu_item"><a asp-area="" asp-controller="Contact" asp-action="Contact">Contacts</a></li>
                    </ul>
                </div>
            </div>

            <div class="menu_close"><i class="fa fa-times" aria-hidden="true"></i></div>

            <div class="menu_social">
                <ul>
                    <li><a href="#"><i class="fa fa-pinterest" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                </ul>
            </div>
        </div>

        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-hidden="true" focusable="false" width="15" height="15" style="-ms-transform: rotate(360deg); -webkit-transform: rotate(360deg); transform: rotate(360deg);" preserveAspectRatio="xMidYMid meet" viewBox="0 0 1024 1024"><path id="facebook-logo-path" d="M880 112H144c-17.7 0-32 14.3-32 32v736c0 17.7 14.3 32 32 32h736c17.7 0 32-14.3 32-32V144c0-17.7-14.3-32-32-32zm-92.4 233.5h-63.9c-50.1 0-59.8 23.8-59.8 58.8v77.1h119.6l-15.6 120.7h-104V912H539.2V602.2H434.9V481.4h104.3v-89c0-103.3 63.1-159.6 155.3-159.6c44.2 0 82.1 3.3 93.2 4.8v107.9z" fill="white" /></svg>

        <div class="modal fade" tabindex="-1" id="loginModal" data-keyboard="false" data-backdrop="static">
            <div class="modal-dialog modal-lg login-dialog">
                <div id="login-content" class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close-button" data-dismiss="modal"></button>
                        <p class="login-title" id="login-title">Create an Account</p>
                        <p class="login-message" id="login-header-msg">By creating an account you'll be able to buy, sell, comment, and more</p>
                    </div>
                    <div class="modal-body">
                        <a class="button login-button facebook" id="facebook-btn" asp-controller="Auth" asp-action="Facebook">
                            <svg class="facebook-logo" focusable="false" width="15" height="15" viewBox="0 0 1024 1024"><path d="M880 112H144c-17.7 0-32 14.3-32 32v736c0 17.7 14.3 32 32 32h736c17.7 0 32-14.3 32-32V144c0-17.7-14.3-32-32-32zm-92.4 233.5h-63.9c-50.1 0-59.8 23.8-59.8 58.8v77.1h119.6l-15.6 120.7h-104V912H539.2V602.2H434.9V481.4h104.3v-89c0-103.3 63.1-159.6 155.3-159.6c44.2 0 82.1 3.3 93.2 4.8v107.9z" fill="white" /></svg>
                            <span id="facebook-btn-caption">Sign up with Facebook</span>
                        </a>
                        <a class="button login-button google" id="google-btn" asp-controller="Auth" asp-action="Google">
                            <svg class="google-logo" aria-hidden="true" focusable="false" width="15" height="15" viewBox="0 0 48 48"><path fill="#FFC107" d="M43.611 20.083H42V20H24v8h11.303c-1.649 4.657-6.08 8-11.303 8c-6.627 0-12-5.373-12-12s5.373-12 12-12c3.059 0 5.842 1.154 7.961 3.039l5.657-5.657C34.046 6.053 29.268 4 24 4C12.955 4 4 12.955 4 24s8.955 20 20 20s20-8.955 20-20c0-1.341-.138-2.65-.389-3.917z" /><path fill="#FF3D00" d="M6.306 14.691l6.571 4.819C14.655 15.108 18.961 12 24 12c3.059 0 5.842 1.154 7.961 3.039l5.657-5.657C34.046 6.053 29.268 4 24 4C16.318 4 9.656 8.337 6.306 14.691z" /><path fill="#4CAF50" d="M24 44c5.166 0 9.86-1.977 13.409-5.192l-6.19-5.238A11.91 11.91 0 0 1 24 36c-5.202 0-9.619-3.317-11.283-7.946l-6.522 5.025C9.505 39.556 16.227 44 24 44z" /><path fill="#1976D2" d="M43.611 20.083H42V20H24v8h11.303a12.04 12.04 0 0 1-4.087 5.571l.003-.002l6.19 5.238C36.971 39.205 44 34 44 24c0-1.341-.138-2.65-.389-3.917z"/></svg>
                            <span id="google-btn-caption">Sign up with Google</span>
                        </a>
                        <hr>
                        <button class="button login-button" id="email-btn"><span id="email-btn-caption">Sign up with Email</span></button>
                    </div>
                    <div class="modal-footer">
                        <p class="login-message" id="login-footer-msg">
                            Already have an account? <a>Log in</a>
                        </p>
                        <p class="login-privacy" id="login-privacy">
                            By creating an account, I accept Sneaker's
                            <a asp-controller="Home" asp-action="Privacy">Terms of Services</a>
                            and <a asp-controller="Home" asp-action="Privacy">Privacy Policy</a>.
                        </p>
                    </div>
                </div>
                <div id="auth-content" class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">x</button>
                        <p class="login-title" id="auth-title">Create an Account</p>
                    </div>
                    <div class="modal-body">
                        <form id="auth-form" class="auth-form">
                            <div class="auth-input">
                                <label for="email-input">Email Address</label>
                                <input type="email" id="email-input" class="email-input" required="required">
                            </div>
                            <div class="auth-input">
                                <label for="password-input">Password</label>
                                <input type="password" id="password-input" class="password-input" required="required">
                            </div>
                            <div class="notify-input" id="notify-footer-msg">
                                <input type="checkbox" id="notification-checkbox" name="regular_checkbox" class="regular_checkbox">
                                <label for="notification-checkbox" class="checkbox_title">Sign up for emails from Sneaker.</label>
                            </div>
                            <button class="button login-button auth-button" id="auth-btn"><span id="auth-btn-caption">Sign up</span></button>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <p class="login-message" id="auth-footer-msg">
                            <a>Forgot your password?</a>
                        </p>
                        <p class="login-privacy" id="auth-privacy">
                            By creating an account, I accept Sneaker's
                            <a asp-controller="Home" asp-action="Privacy">Terms of Services</a>
                            and <a asp-controller="Home" asp-action="Privacy">Privacy Policy</a>.
                        </p>
                    </div>
                </div>
            </div>
        </div>

        @RenderBody()
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/css/bootstrap4/popper.js"></script>
    <script src="~/css/bootstrap4/bootstrap.min.js"></script>
    <script src="~/lib/greensock/TweenMax.min.js"></script>
    <script src="~/lib/greensock/TimelineMax.min.js"></script>
    <script src="~/lib/greensock/animation.gsap.min.js"></script>
    <script src="~/lib/Isotope/isotope.pkgd.min.js"></script>
    <script src="~/lib/easing/easing.js"></script>
    <script src="~/lib/parallax-js-master/parallax.min.js"></script>
    <script src="~/js/layout.js"></script>
    <script src="~/js/service.js"></script>
    @RenderSection("Scripts", required: false)
    <script>
        $(".facebook-logo-svg").html('<path class="facebook" d="M880 112H144c-17.7 0-32 14.3-32 32v736c0 17.7 14.3 32 32 32h736c17.7 0 32-14.3 32-32V144c0-17.7-14.3-32-32-32zm-92.4 233.5h-63.9c-50.1 0-59.8 23.8-59.8 58.8v77.1h119.6l-15.6 120.7h-104V912H539.2V602.2H434.9V481.4h104.3v-89c0-103.3 63.1-159.6 155.3-159.6c44.2 0 82.1 3.3 93.2 4.8v107.9z" fill="white"/>')
    </script>

<div class="footer_overlay"></div>
    <footer class="footer">
        @*<div class="footer_background" style="background-image: url(/images/footer.jpeg)"></div>*@
        <div class="container">
            <div class="row">
                <div class="col">
                    <div class="footer_content d-flex flex-lg-row flex-column align-items-center justify-content-lg-start justify-content-center">
                        <div class="footer_logo"><a href="#">Sneaker.</a></div>
                        <div class="copyright ml-auto mr-auto">
                            Copyright &copy;
                            <script>document.write(new Date().getFullYear());</script> All rights reserved
                        </div>
                        <div class="footer_social ml-lg-auto">
                            <ul>
                                <li><a href="#"><i class="fa fa-pinterest" aria-hidden="true"></i></a></li>
                                <li><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                                <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
</body>
</html>